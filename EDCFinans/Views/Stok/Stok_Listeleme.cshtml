@model IEnumerable<EDCFinans.Models.Finans.Stok>

@{
    ViewData["Title"] = "Stok_Listeleme";
}

<h1>Stok_Listeleme</h1>

<p>
    <a asp-action="Stok_Ekle">Yeni Kayıt</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UrunDetayId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DepoId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Adet)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UrunDetayId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DepoId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adet)
            </td>
            <td>
                @Html.ActionLink("Düzenle", "Stok_Duzenle", new {  id=item.Id }) |
            </td>
        </tr>
}
    </tbody>
</table>
